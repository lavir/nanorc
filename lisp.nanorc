syntax "lisp" "(emacs|zile)$" "\.(el|li?sp|scm|ss)$"

FUNCTION: "\([a-z-]+"
OPERATOR: "\(([-+*/<>]|<=|>=)|'"
PLAIN:    "\("
NUMBER:   "\b[0-9]+\b"
~KEYWORD: "\bnil\b"
BOOLEAN:  "\b[tT]\b"
STRING:   "\"(\\.|[^"])*\""
ESCAPE:   "'[A-Za-z][A-Za-z0-9_-]+"
ESCAPE:   "\\.?"
COMMENT:  "(^|[[:space:]]);.*"
+LINT
