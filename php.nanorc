syntax "php" "\.php$"

OPERATOR: "[-+/*=<>?:!~%&|]"
SECTION:  "(<\?(php)?|\?>)"
+FUNCTION
TYPE:     "\b(float|global|double|bool|char|int|enum|const)\b"
KEYWORD:  "\b(class|new|private|public|function|for|foreach|if|while|do|var)\b"
KEYWORD:  "\b(return|else|elseif|case|default|switch)\b"
JUMP:     "\b(goto|break|continue)\b"
+BOOLEAN
+STRING
VARIABLE: "\$[A-Za-z_][A-Za-z0-9_]*"
+CCOMMENT
+TODO
+LINT
